<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.gatech.rts.ezfair"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <android:uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />
    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <android:uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="edu.gatech.rts.ezfair.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name="edu.gatech.rts.ezfair.JoinedQueueActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="edu.gatech.rts.ezfair.MainActivity"
            android:label="EZFair" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="edu.gatech.rts.ezfair.OtherQueueActivity"
            android:label="@string/title_activity_other_queue" >
        </activity>

        <service
            android:name="edu.gatech.rts.ezfair.JoinPhysicalQueueService"
            android:enabled="true"
            android:exported="true" >
        </service>
        <service
            android:name="edu.gatech.rts.ezfair.LeavePhysicalQueueService"
            android:enabled="true"
            android:exported="true" >
        </service>
    </application>

</manifest><!--<?xml version="1.0" encoding="utf-8"?>-->
